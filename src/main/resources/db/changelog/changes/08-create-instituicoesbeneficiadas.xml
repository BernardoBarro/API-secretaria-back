<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
	<changeSet id="08_create_instituicoes_beneficiadas" author="joao">
		<sql>
	CREATE TABLE INSTITUICOES_BENEFICIADAS (
    ID_INSTITUICAO BIGINT NOT NULL AUTO_INCREMENT,
    NOME_INSTITUICAO VARCHAR(50),
    CONTATO VARCHAR(11),
    DETALHES VARCHAR,
    PRIMARY KEY (ID_INSTITUICAO)
);

		</sql>
		<rollback>
			<sql>
				DROP TABLE INSTITUICOES_BENEFICIADAS;
			</sql>
		</rollback>
	</changeSet>
</databaseChangeLog>